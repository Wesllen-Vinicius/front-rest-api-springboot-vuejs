    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "test:unit": "cypress open --component",
    "test:unit:ci": "cypress run --component --quiet --reporter spec"
  },
  "dependencies": {
