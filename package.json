    "vue": "^3.2.36",
    "vue-router": "^4.0.15",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.0.2",
    "postcss": "^8.4.14",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.3",
    "vee-validate": "^4.5.11",
    "vite": "^2.9.9",
    "yup": "^0.32.11"
  }
